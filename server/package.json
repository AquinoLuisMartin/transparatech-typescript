{
  "name": "transparatech-server",
  "version": "1.0.0",
  "description": "Backend server for Transparatech application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "node database/setup.js migrate",
    "db:seed": "node database/setup.js seed",
    "db:reset": "node database/setup.js reset",
    "security:setup": "node src/scripts/setup-security.js --generate",
    "security:validate": "node src/scripts/setup-security.js --validate",
    "security:jwt": "node src/scripts/setup-security.js --jwt-secret",
    "prestart": "node src/scripts/setup-security.js --validate"
  },
  "keywords": [
    "express",
    "postgresql",
    "rest-api",
    "transparatech"
  ],
  "author": "Transparatech Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@google/genai": "^1.28.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10"
  }
}
